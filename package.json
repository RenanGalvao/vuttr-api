{
  "name": "vuttr-api",
  "version": "1.0.0",
  "description": "Very Useful Tools to Remember is an API to save your favorites tools made in NodeJS.",
  "main": "dist/app.js",
  "author": "Renan Machado",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node dist/app",
    "dev": "NODE_ENV=development ./node_modules/.bin/ts-node-dev --transpile-only --ignore-watch node_modules src/app.ts",
    "test": "NODE_ENV=test mocha --recursive --exit -r ts-node/register \"./test/**/*.ts\"",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/express": "4.17.9",
    "@types/jsonwebtoken": "8.5.0",
    "@types/mocha": "8.0.4",
    "@types/mongoose": "5.10.0",
    "@types/yup": "0.29.9",
    "@typescript-eslint/eslint-plugin": "4.8.0",
    "@typescript-eslint/parser": "4.8.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "eslint": "7.13.0",
    "mocha": "8.2.1",
    "ts-node": "9.0.0",
    "ts-node-dev": "1.0.0",
    "typescript": "4.0.5"
  },
  "dependencies": {
    "bcrypt": "5.0.0",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "helmet": "4.2.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.10.15",
    "yup": "0.29.3"
  }
}
