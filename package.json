    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "eslint": "7.13.0",
    "mocha": "8.2.1",
    "ts-node-dev": "1.0.0",
    "typescript": "4.0.5"
  },
  "dependencies": {
    "connect-mongo": "3.2.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "helmet": "4.2.0",
    "mongoose": "5.10.15",
    "yup": "0.29.3"
